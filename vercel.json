{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(login|register|checkout|keranjang|produk|tentang|kontak|dashboard)",
      "dest": "/api/$1.php"
    },
    {
      "src": "/api/(.*).php",
      "dest": "/api/$1.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}